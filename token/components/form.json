      "type": "composition"
    },
    "container": {
      "check": {
        "active": {
          "value": {
            "height": "{size-checkbox.width}",
            "width": "{size-checkbox.width}",
            "fill": "#056AF9",
            "borderWidth": "0",
            "borderRadius": "{br-checkbox}",
            "borderColor": "{fg.form-elem.border}",
            "boxShadow": "{shadow.none}"
          },
          "type": "composition"
        },
        "active-hover": {
          "value": {
            "height": "{size-checkbox.width}",
            "width": "{size-checkbox.width}",
            "fill": "#0452BC",
            "borderWidth": "0",
            "borderRadius": "{br-checkbox}",
            "borderColor": "{fg.form-elem.border}",
            "boxShadow": "{shadow.primary}"
          },
          "type": "composition"
        },
        "inactive": {
          "value": {
            "height": "{size-checkbox.width}",
            "width": "{size-checkbox.width}",
            "fill": "{fg.text.inverted}",
            "borderWidth": "{bw.thin}",
            "borderRadius": "{br-checkbox}",
            "borderColor": "{fg.form-elem.border}",
            "boxShadow": "{shadow.none}"
          },
          "type": "composition"
        },
        "inactive-hover": {
          "value": {
