{
  "radio": {
    "button": {
      "inactive": {
        "value": {
          "width": "{size-radio-button.circumfrence}",
          "height": "{size-radio-button.circumfrence}",
          "fill": "{fg.text.inverted}",
          "borderColor": "{fg.form-elem.border}",
          "borderWidth": "{bw.med}",
          "boxShadow": "{shadow.none}"
        },
        "type": "composition"
      },
      "inactive-hover": {
        "value": {
          "width": "{size-radio-button.circumfrence}",
          "height": "{size-radio-button.circumfrence}",
          "fill": "{fg.text.inverted}",
          "borderColor": "{fg.form-elem.border}",
          "borderWidth": "{bw.med}",
          "boxShadow": "{shadow.primary}"
        },
        "type": "composition"
      },
      "active": {
        "value": {
          "width": "{size-radio-button.circumfrence}",
          "height": "{size-radio-button.circumfrence}",
          "fill": "{fg.text.inverted}",
          "borderColor": "{fg.form-elem.active}",
          "borderWidth": "{bw.thick}",
          "boxShadow": "{shadow.none}"
        },
        "type": "composition"
      },
      "active-hover": {
        "value": {
          "width": "{size-radio-button.circumfrence}",
          "height": "{size-radio-button.circumfrence}",
          "fill": "{fg.text.inverted}",
          "borderColor": "{fg.form-elem.active}",
          "borderWidth": "{bw.thick}",
          "boxShadow": "{shadow.primary}"
        },
        "type": "composition"
      }
    },
    "wrapper": {
      "value": {
        "itemSpacing": "{4u}"
      },
      "type": "composition"
    },
    "label": {
      "value": {
        "typography": "{label.default}",
        "fill": "{fg.text.primary}"
      },
      "type": "composition"
    },
    "component-wrapper": {
      "value": {
        "height": "{size-radio-button.circumfrence}"
      },
      "type": "composition"
    }
  },
  "toggle": {
    "wrapper": {
      "value": {
        "itemSpacing": "{4u}"
      },
      "type": "composition"
    },
    "label": {
      "value": {
        "typography": "{label.default}",
        "fill": "{fg.text.primary}"
      },
      "type": "composition"
    },
    "container": {
      "inactive": {
        "value": {
          "width": "({size-toggle.indicator.circumfrence}*2)+2*{base}",
          "verticalPadding": "{toggle.container.padding}",
          "horizontalPadding": "{toggle.container.padding}",
          "fill": "{fg.form-elem.inactive}",
          "borderRadius": "9999",
          "boxShadow": "{shadow.none}"
        },
        "type": "composition"
      },
      "active": {
        "value": {
          "width": "({size-toggle.indicator.circumfrence}*2)+2*{base}",
          "verticalPadding": "{toggle.container.padding}",
          "horizontalPadding": "{toggle.container.padding}",
          "fill": "{fg.form-elem.active}",
          "borderRadius": "9999",
          "boxShadow": "{shadow.none}"
        },
        "type": "composition"
      },
      "active-hover": {
        "value": {
          "width": "({size-toggle.indicator.circumfrence}*2)+2*{base}",
          "verticalPadding": "{toggle.container.padding}",
          "horizontalPadding": "{toggle.container.padding}",
          "fill": "{fg.form-elem.active}",
          "borderRadius": "9999",
          "boxShadow": "{shadow.primary}"
        },
        "type": "composition"
      },
      "inactive-hover": {
        "value": {
          "width": "({size-toggle.indicator.circumfrence}*2)+2*{base}",
          "verticalPadding": "{toggle.container.padding}",
          "horizontalPadding": "{toggle.container.padding}",
          "fill": "{fg.form-elem.inactive}",
          "borderRadius": "9999",
          "boxShadow": "{shadow.primary}"
        },
        "type": "composition"
      },
      "padding": {
        "value": "{base}/2",
        "type": "spacing"
      }
    },
    "indicator": {
      "value": {
        "width": "{size-toggle.indicator.circumfrence}",
        "height": "{size-toggle.indicator.circumfrence}",
        "fill": "{fg.text.inverted}"
      },
      "type": "composition"
    },
    "componenet-wrapper": {
      "value": {
        "height": "{size-toggle.wrapper.height}"
      },
      "type": "composition"
    }
  },
  "size-radio-button": {
    "circumfrence": {
      "value": "{base}*5",
      "type": "sizing"
    }
  },
  "size-toggle": {
    "wrapper": {
      "width": {
        "value": "{base}*12",
        "type": "sizing"
      },
      "height": {
        "value": "{base}*6",
        "type": "sizing"
      }
    },
    "indicator": {
      "circumfrence": {
        "value": "{base}*5",
        "type": "sizing"
      }
    }
  },
  "checkbox": {
    "componenet-wrapper": {
      "value": {
        "height": "{size-checkbox.width}"
      },
      "type": "composition"
    },
    "container": {
      "check": {
        "active": {
          "value": {
            "height": "{size-checkbox.width}",
            "width": "{size-checkbox.width}",
            "fill": "#056AF9",
            "borderWidth": "0",
            "borderRadius": "4",
            "borderColor": "{fg.form-elem.border}",
            "boxShadow": "{shadow.none}"
          },
          "type": "composition"
        },
        "active-hover": {
          "value": {
            "height": "{size-checkbox.width}",
            "width": "{size-checkbox.width}",
            "fill": "#0452BC",
            "borderWidth": "0",
            "borderRadius": "4",
            "borderColor": "{fg.form-elem.border}",
            "boxShadow": "{shadow.primary}"
          },
          "type": "composition"
        },
        "inactive": {
          "value": {
            "height": "{size-checkbox.width}",
            "width": "{size-checkbox.width}",
            "fill": "{fg.text.inverted}",
            "borderWidth": "{bw.thin}",
            "borderRadius": "4",
            "borderColor": "{fg.form-elem.border}",
            "boxShadow": "{shadow.none}"
          },
          "type": "composition"
        },
        "inactive-hover": {
          "value": {
            "height": "{size-checkbox.width}",
            "width": "{size-checkbox.width}",
            "fill": "{fg.text.inverted}",
            "borderWidth": "{bw.thin}",
            "borderRadius": "4",
            "borderColor": "{fg.form-elem.border}",
            "boxShadow": "{shadow.primary}"
          },
          "type": "composition"
        }
      },
      "x": {
        "active": {
          "value": {
            "height": "{size-checkbox.width}",
            "width": "{size-checkbox.width}",
            "fill": "rgba({fg.text.inverted},0)",
            "borderWidth": "0",
            "borderRadius": "4",
            "borderColor": "{fg.form-elem.border}",
            "boxShadow": "{shadow.none}"
          },
          "type": "composition"
        },
        "active-hover": {
          "value": {
            "height": "{size-checkbox.width}",
            "width": "{size-checkbox.width}",
            "fill": "{fg.text.inverted}",
            "borderWidth": "0",
            "borderRadius": "4",
            "borderColor": "{fg.form-elem.border}",
            "boxShadow": "{shadow.primary}"
          },
          "type": "composition"
        },
        "inactive": {
          "value": {
            "height": "{size-checkbox.width}",
            "width": "{size-checkbox.width}",
            "fill": "{fg.text.inverted}",
            "borderWidth": "{bw.thin}",
            "borderRadius": "4",
            "borderColor": "{fg.form-elem.border}",
            "boxShadow": "{shadow.none}"
          },
          "type": "composition"
        },
        "inactive-hover": {
          "value": {
            "height": "{size-checkbox.width}",
            "width": "{size-checkbox.width}",
            "fill": "{fg.text.inverted}",
            "borderWidth": "{bw.thin}",
            "borderRadius": "4",
            "borderColor": "{fg.form-elem.border}",
            "boxShadow": "{shadow.primary}"
          },
          "type": "composition"
        }
      }
    }
  },
  "size-checkbox": {
    "width": {
      "value": "{base}*5",
      "type": "sizing"
    }
  }
}