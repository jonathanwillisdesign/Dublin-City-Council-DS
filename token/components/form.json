    "wrapper": {
      "value": {
        "itemSpacing": "{4u}"
      },
      "type": "composition"
    },
    "label": {
      "value": {
        "typography": "{label.default}",
        "fill": "{fg.text.primary}"
      },
      "type": "composition"
    },
    "component-wrapper": {
      "value": {
        "height": "{size-radio-button.circumfrence}"
      },
      "type": "composition"
    }
  },
  "toggle": {
    "wrapper": {
      "value": {
        "itemSpacing": "{4u}"
      },
      "type": "composition"
    },
    "label": {
      "value": {
        "typography": "{label.default}",
        "fill": "{fg.text.primary}"
      },
      "type": "composition"
    },
    "container": {
      "inactive": {
        "value": {
          "width": "({size-toggle.indicator.circumfrence}*2)+2*{base}",
          "verticalPadding": "{toggle.container.padding}",
          "horizontalPadding": "{toggle.container.padding}",
          "fill": "{fg.form-elem.inactive}",
          "borderRadius": "9999",
          "boxShadow": "{shadow.none}"
        },
        "type": "composition"
      },
      "active": {
        "value": {
          "width": "({size-toggle.indicator.circumfrence}*2)+2*{base}",
          "verticalPadding": "{toggle.container.padding}",
          "horizontalPadding": "{toggle.container.padding}",
          "fill": "{fg.form-elem.active}",
          "borderRadius": "9999",
          "boxShadow": "{shadow.none}"
        },
        "type": "composition"
      },
      "active-hover": {
        "value": {
          "width": "({size-toggle.indicator.circumfrence}*2)+2*{base}",
          "verticalPadding": "{toggle.container.padding}",
          "horizontalPadding": "{toggle.container.padding}",
          "fill": "{fg.form-elem.active}",
          "borderRadius": "9999",
          "boxShadow": "{shadow.primary}"
        },
        "type": "composition"
      },
      "inactive-hover": {
        "value": {
          "width": "({size-toggle.indicator.circumfrence}*2)+2*{base}",
          "verticalPadding": "{toggle.container.padding}",
          "horizontalPadding": "{toggle.container.padding}",
          "fill": "{fg.form-elem.inactive}",
          "borderRadius": "9999",
          "boxShadow": "{shadow.primary}"
        },
        "type": "composition"
      },
      "padding": {
        "value": "{base}/2",
        "type": "spacing"
      }
    },
    "indicator": {
      "value": {
        "width": "{size-toggle.indicator.circumfrence}",
        "height": "{size-toggle.indicator.circumfrence}",
        "fill": "{fg.text.inverted}"
      },
      "type": "composition"
    },
    "componenet-wrapper": {
      "value": {
        "height": "{size-toggle.wrapper.height}"
      },
      "type": "composition"
    }
  },
  "size-radio-button": {
    "circumfrence": {
      "value": "{base}*5",
      "type": "sizing"
    }
  },
  "size-toggle": {
    "wrapper": {
      "width": {
        "value": "{base}*12",
        "type": "sizing"
      },
      "height": {
        "value": "{base}*6",
        "type": "sizing"
      }
    },
    "indicator": {
      "circumfrence": {
        "value": "{base}*5",
        "type": "sizing"
      }
    }
  },
  "checkbox": {
