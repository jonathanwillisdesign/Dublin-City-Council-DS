            "width": "{size-checkbox.width}",
            "fill": "{fg.text.inverted}",
            "borderWidth": "{bw.thin}",
            "borderRadius": "{br-checkbox}",
            "borderColor": "{fg.form-elem.border}",
            "boxShadow": "{shadow.primary}"
          },
          "type": "composition"
        }
      },
      "x": {
        "active": {
          "value": {
            "height": "{size-checkbox.width}",
            "width": "{size-checkbox.width}",
            "fill": "rgba({fg.text.inverted},0)",
            "borderWidth": "0",
            "borderRadius": "{br-checkbox}",
            "borderColor": "{fg.form-elem.border}",
            "boxShadow": "{shadow.none}"
          },
          "type": "composition"
        },
        "active-hover": {
          "value": {
            "height": "{size-checkbox.width}",
            "width": "{size-checkbox.width}",
            "fill": "{fg.text.inverted}",
            "borderWidth": "0",
            "borderRadius": "{br-checkbox}",
            "borderColor": "{fg.form-elem.border}",
            "boxShadow": "{shadow.primary}"
          },
          "type": "composition"
        },
        "inactive": {
          "value": {
            "height": "{size-checkbox.width}",
            "width": "{size-checkbox.width}",
            "fill": "{fg.text.inverted}",
            "borderWidth": "{bw.thin}",
            "borderRadius": "{br-checkbox}",
            "borderColor": "{fg.form-elem.border}",
            "boxShadow": "{shadow.none}"
          },
          "type": "composition"
        },
        "inactive-hover": {
          "value": {
            "height": "{size-checkbox.width}",
            "width": "{size-checkbox.width}",
            "fill": "{fg.text.inverted}",
            "borderWidth": "{bw.thin}",
            "borderRadius": "{br-checkbox}",
            "borderColor": "{fg.form-elem.border}",
            "boxShadow": "{shadow.primary}"
          },
          "type": "composition"
        }
      }
    },
    "icon-wrapper": {
      "value": {
        "width": "{size-checkbox.icon}",
        "height": "{size-checkbox.icon}"
      },
      "type": "composition"
    },
    "icon": {
      "check": {
        "value": {
          "border": "{icon.border.default.inverted}"
        },
        "type": "composition"
      },
      "X": {
        "value": {
          "border": "{icon.border.default.primary}"
        },
        "type": "composition"
      }
    }
  },
  "size-checkbox": {
    "width": {
      "value": "{base}*5",
      "type": "sizing"
    },
    "icon": {
      "value": "{base}*4",
      "type": "sizing"
    }
  },
  "br-checkbox": {
    "value": "{base}",
    "type": "borderRadius"
  },
  "bw-checkbox": {
    "value": "{bw.thin}",
